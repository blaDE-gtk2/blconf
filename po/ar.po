#: ../blconfd/blconf-backend.c:103
#, c-format
msgid "Property names must start with a '/' character"
msgstr "أسماء الخصائص يجب ان تبدء بالرمز '/'"

#: ../blconfd/blconf-backend.c:112
#, c-format
msgid "The root element ('/') is not a valid property name"
msgstr "عنصر الجذر ('/') ليس إسم خصائص سليم"

#: ../blconfd/blconf-backend.c:126
#, c-format
msgid ""
"Property names can only include the ASCII characters A-Z, a-z, 0-9, '_', "
"'-', '<' and '>', as well as '/' as a separator"
msgstr "يمكن أن تتضمن أسماء الخصائص فقط الأحرف A-Z, 0-9, '_', '-', '<' and '>', as فضلا عن '/' كفاصل"

#: ../blconfd/blconf-backend.c:135
#, c-format
msgid "Property names cannot have two or more consecutive '/' characters"
msgstr "أسماء الخصائص لا يمكن ان تحتوي على رمزين او اكثر متتالية من '/'"

#: ../blconfd/blconf-backend.c:146
#, c-format
msgid "Property names cannot end with a '/' character"
msgstr "أسماء الخصائص لا يمكن ان تنتهي بالرمز '/'"

#: ../blconfd/blconf-backend.c:163
#, c-format
msgid "Channel name cannot be an empty string"
msgstr "إسم القناة لا يمكن ان يكون فارغ"

#: ../blconfd/blconf-backend.c:176
#, c-format
msgid ""
"Channel names can only include the ASCII characters A-Z, a-z, 0-9, '_', and "
"'-'"
msgstr "يمكن لأسماء القنوات ان تحتوي فقط على رموز ASCII وهي A-Z,0-9, '_', و '-'"

#: ../blconfd/blconf-backend.c:398
#, c-format
msgid ""
"The property name can only be empty or \"/\" if a recursive reset was "
"specified"
msgstr "يمكن لإسم الخصائص أن يكون فارغا أو \"/\" إذا تمت إعادة تعيين متكررة"

#: ../blconfd/main.c:134
msgid "Prints the blconfd version."
msgstr "طباعة نسخة blconfd."

#: ../blconfd/main.c:136
