#: ../blconf-query/main.c:530
msgid "Failed to set property"
msgstr "No se pudo establecer la propiedad."

#: ../blconf-query/main.c:360
msgid "--toggle only works with boolean values"
msgstr "--toggle únicamente funciona con valores booleanos"

#: ../blconf-query/main.c:369 ../blconf-query/main.c:388
msgid "(unknown)"
msgstr "(desconocido)"

